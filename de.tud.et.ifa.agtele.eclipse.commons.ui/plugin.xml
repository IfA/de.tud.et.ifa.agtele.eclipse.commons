<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="de.tud.et.ifa.agtele.ui.views.category"
            name="%_UI_ViewCategory_Name">
      </category>
      <view
            category="de.tud.et.ifa.agtele.ui.views.category"
            class="de.tud.et.ifa.agtele.ui.views.EMFModelHelpView"
            icon="icons/help_view.gif"
            id="de.tud.et.ifa.agtele.ui.views.EMFModelHelpView"
            name="%_UI_EMFModelHelpView_Name">
      </view>
   </extension>   
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="de.tud.et.ifa.agtele.ui.views.EMFModelHelpView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
</plugin>
